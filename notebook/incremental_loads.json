					"AND source.procedure_reference = target.procedure_reference\r\n",
					"AND source.procedure_sequence = target.procedure_sequence\r\n",
					"WHEN MATCHED THEN\r\n",
					"UPDATE SET\r\n",
					"target.Claim_ID = source.Claim_ID\r\n",
					",target.procedure_reference = source.procedure_reference\r\n",
					",target.procedure_sequence = source.procedure_sequence\r\n",
					"WHEN NOT MATCHED THEN\r\n",
					"INSERT (target.Claim_ID \r\n",
					",target.procedure_reference\r\n",
					",target.procedure_sequence)\r\n",
					"VALUES (source.Claim_ID\r\n",
					",source.procedure_reference\r\n",
